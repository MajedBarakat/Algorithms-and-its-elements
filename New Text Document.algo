ALGORITHM algorithm_name
VAR
    x:CHAR
    a,b,c:INTEGER
BEGIN
    read (x)
    a:=0
    b:=1
    c:=0
    WHILE (x <> '.') DO
        a=a+1
        
        IF (x=" ") THEN
        b=b+1
        END_IF
        
        IF (x="a" OR x="e" OR x="i" OR x="y" OR x="u" OR X="A" OR X="E" OR X="I" OR X="Y" OR X="U") THEN
        c=c+1
        END_IF

    END_WHILE
END